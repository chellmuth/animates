<div class="row text-center">
  <svg
    width={{width}}
    height={{height}}
    xmlns="http://www.w3.org/2000/svg"
    version="1.1"
  ></svg>
</div>
<div class="row text-center">
  <div class="btn-group">
    {{#each frame in frames}}
      <button type="button" class="btn btn-primary">{{frame}}</button>
    {{/each}}
  </div>
</div>
